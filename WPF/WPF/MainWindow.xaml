        <Button Content="Button" HorizontalAlignment="Left" Margin="445,166,0,0" VerticalAlignment="Top" Width="163" Height="52" Click="Button_Click"/>
        <Label Content="Name:" HorizontalAlignment="Left" Margin="55,123,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.024,-4.593"/>
        <Label Content="GPA" HorizontalAlignment="Left" Margin="67,162,0,0" VerticalAlignment="Top"/>
        <Label Content="DOB" HorizontalAlignment="Left" Margin="67,192,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="id" HorizontalAlignment="Left" Height="23" Margin="167,92,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="dob" HorizontalAlignment="Left" Height="23" Margin="167,209,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="gpa" HorizontalAlignment="Left" Height="23" Margin="167,166,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="name" HorizontalAlignment="Left" Height="22" Margin="167,127,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="output" HorizontalAlignment="Left" Height="93" Margin="416,48,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="273" RenderTransformOrigin="0.5,0.5" TextChanged="TextBox_TextChanged">
